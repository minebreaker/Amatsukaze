{
    "name": "amatsukaze",
    "version": "0.1.0",
    "description": "",
    "repository": "",
    "main": "index.js",
    "type": "type/index.d.ts",
    "scripts": {
        "clean": "rimraf out/*",
        "build": "npm run bundle && npm run test && npm run declaration",
        "bundle": "rollup --config",
        "declaration": "tsc --outDir type --newLine LF --lib es6 --target es6 --module es6 --moduleResolution node --declaration --emitDeclarationOnly src/index.ts",
        "bundleTest": "rollup --config rollup.test.config.js",
        "testModule": "npm run bundleTest && mocha out/test-all.js",
        "test": "npm run testModule && npm run testCompilation",
        "compileTestCompilation": "tsc test/test-compilation.ts --lib es6 --target es6 --module none --moduleResolution node --outDir ./out",
        "testCompilation": "npm run compileTestCompilation && node out/test-compilation.js"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "immutable": "^4.0.0-rc.12"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/lodash": "^4.14.123",
        "@types/mocha": "^5.2.6",
        "chai": "^4.2.0",
        "mocha": "^6.1.4",
        "rimraf": "^2.6.3",
        "rollup": "^1.10.1",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-typescript": "^1.0.1",
        "tslint": "^5.16.0",
        "typescript": "^3.4.4"
    }
}
